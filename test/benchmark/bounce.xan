class Random {
	init() {
		this.seed = 74755;
	}

	next() {
		this.seed = ((this.seed * 1309) + 13849) % 65536;
		return this.seed;
	}
}

var a = Random();
for(var i = 0; i < 9; i = i + 1)
	print(a.next());
